package com.example.rfid.vo
import com.example.rfid.vo.People
dialect  "java"

rule "man"
    when
        $p : People(sex == 1)
    then
        System.out.println($p.getName() + "是男孩");
end

rule "girl"
    when
        $p : People(sex == 0)
    then
        System.out.println($p.getName() + "是女孩");
end